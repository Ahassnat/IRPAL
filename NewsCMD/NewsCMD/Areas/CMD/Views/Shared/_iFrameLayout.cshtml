@{ 
    var db = ViewBag.db as NewsCMD.Models.FBaselNewsWebSiteEntities;
}
<!DOCTYPE html>

<html lang="en" dir="rtl">

<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title | لوحة التحكم </title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta http-equiv="Content-type" content="text/html; charset=utf-8">
    <meta content="" name="description" />
    <meta content="" name="author" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="https://fonts.googleapis.com/css?family=Amiri|Cairo" rel="stylesheet">
    <link href="/Content/metronic V4-rtl/assets/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/metronic V4-rtl/assets/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/metronic V4-rtl/assets/global/plugins/bootstrap/css/bootstrap-rtl.min.css" rel="stylesheet" type="text/css" />
    @*<link href="/Content/metronic V4-rtl/assets/global/plugins/uniform/css/uniform.default.css" rel="stylesheet" type="text/css" />*@
    <link href="/Content/metronic V4-rtl/assets/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css" rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN THEME STYLES -->
    <link href="/Content/metronic V4-rtl/assets/global/css/components-md-rtl.css" id="style_components" rel="stylesheet" type="text/css" />
    <link href="/Content/metronic V4-rtl/assets/global/css/plugins-md-rtl.css" rel="stylesheet" type="text/css" />
    <link href="/Content/metronic V4-rtl/assets/admin/layout/css/layout-rtl.css" rel="stylesheet" type="text/css" />
    <link id="style_color" href="/Content/metronic V4-rtl/assets/admin/layout/css/themes/darkblue-rtl.css" rel="stylesheet" type="text/css" />
    <link href="/Content/metronic V4-rtl/assets/admin/layout/css/custom-rtl.css" rel="stylesheet" type="text/css" />
    <!-- END THEME STYLES -->
    <link rel="shortcut icon" href="favicon.ico" />
    @RenderSection("css",false)
</head>

<body >
   
    <div class="clearfix">
    </div>
    <!-- BEGIN CONTAINER -->
    <div class="page-container">
       
        <div class="page-content-wrapper">
            <div class="page-content">
                <div class="row">
                    <div class="col-md-12">
                        @Html.Partial("_msg")
                        @Html.Partial("_ConfirmBox")
                        @Html.Partial("iFrame")
                        @RenderBody()
                    </div>
                </div>
                <!-- END PAGE CONTENT-->
            </div>
        </div>
    </div>
    <!-- END CONTAINER -->
    <!-- BEGIN FOOTER -->
   

    <script src="/Content/metronic V4-rtl/assets/global/plugins/jquery.min.js" type="text/javascript"></script>
    <script src="/Content/metronic V4-rtl/assets/global/plugins/jquery-migrate.min.js" type="text/javascript"></script>
    <!-- IMPORTANT! Load jquery-ui.min.js before bootstrap.min.js to fix bootstrap tooltip conflict with jquery ui tooltip -->
    <script src="/Content/metronic V4-rtl/assets/global/plugins/jquery-ui/jquery-ui.min.js" type="text/javascript"></script>
    <script src="/Content/metronic V4-rtl/assets/global/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/Content/metronic V4-rtl/assets/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js" type="text/javascript"></script>
    <script src="/Content/metronic V4-rtl/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="/Content/metronic V4-rtl/assets/global/plugins/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="/Content/metronic V4-rtl/assets/global/plugins/jquery.cokie.min.js" type="text/javascript"></script>
    @*<script src="/Content/metronic V4-rtl/assets/global/plugins/uniform/jquery.uniform.min.js" type="text/javascript"></script>*@
    <script src="/Content/metronic V4-rtl/assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <script src="/Content/metronic V4-rtl/assets/global/scripts/metronic.js" type="text/javascript"></script>
    <script src="/Content/metronic V4-rtl/assets/admin/layout/scripts/layout.js" type="text/javascript"></script>
    <script src="/Content/metronic V4-rtl/assets/admin/layout/scripts/quick-sidebar.js" type="text/javascript"></script>
    <script src="/Content/metronic V4-rtl/assets/admin/layout/scripts/demo.js" type="text/javascript"></script>
    <script>


        jQuery(document).ready(function () {

            $(document).on("click", ".Confirm", function () {
                $("#Confirm").modal("show");
                $("#Confirm .btn-danger").attr("href", $(this).attr("href"));
                return false;

            });

            $(document).on("click", ".iFrame", function () {
                $("#iFrame").modal("show");
                $("#iFrame .modal-title").html($(this).attr("title"));
                $("#iFrame .modal-body").html("<iframe frameborder=0 width=100% height=500 src='"+ $(this).attr("href")+"'></ifame>");
                return false;

            });
            Metronic.init(); // init metronic core components
            Layout.init(); // init current layout
            Demo.init(); // init demo features
            $(".sub-menu li.active").each(function () {
                $(this).parent().parent().addClass("active open");
                $(this).parent().prev().find(".arrow").addClass("open");
                $(this).parent().prev().find(".arrow").before("<span class='selected'></span>");
            });
        });
    </script>
    <!-- END JAVASCRIPTS -->
    @RenderSection("scripts", false)
    
</body>
<!-- END BODY -->
</html>